<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="yll.self.supportdesignlibrarydemo" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TextInputLayoutActivity"/>
        <activity android:name=".FloatingActionButtonActivity"/>
        <activity android:name=".TabActivity"/>
        <activity android:name=".TabLayoutActivity"/>
        <activity android:name=".TabAndViewPagerActivity"/>
        <activity android:name=".NavigationViewActivity"/>
        <activity android:name=".AppBarLayoutActivity"
                  android:theme="@style/NoActionBarBaseTheme"/>
        <activity android:name=".CollapsingToolbarActivity"
                  android:theme="@style/NoActionBarBaseTheme"/>
        <activity android:name=".TransLateBehaviorActivity"/>
        <activity android:name=".ScrollBehaviorActivity"/>
    </application>

</manifest>
